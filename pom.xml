		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.3.5.RELEASE</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.athena.example</groupId>
	<artifactId>spring-learn-1</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>spring-learn-1</name>
	<description>Demo project for Spring Boot</description>


	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>8</java.version>
		<spring.cloud.version>Hoxton.SR6</spring.cloud.version>
		<dubbo.version>2.6.5</dubbo.version>
		<dubbo.spring.version>1.0.2</dubbo.spring.version>
		<sentinel.version>1.4.0</sentinel.version>
		<jax.rs.version>2.1.1</jax.rs.version>
